doctype html
html
    head
        link(rel='stylesheet' href='https://www.w3schools.com/w3css/4/w3.css')
    body
        h2 Error
        ul
            li
                p= message
            if error.status
                li
                    p= error.status
        div(class='w3-container', style='margin: auto; width: 90%; background-color: rgb(239, 239, 239);')
            pre #{error.stack}
